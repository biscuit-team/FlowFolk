<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="interpreterConfig" class="cn.sotou.flowfolk.interpreter.InterpreterConfig">
		<property name="multiThreads">
			<value>false</value>
		</property>
		<property name="maxThreadNum">
			<value>10</value>
		</property>
		<property name="utilProvider">
			<bean class="cn.sotou.flowfolk.util.provider.PipeUtilProvider">
				<property name="utilProviders">
					<list>
						<bean class="cn.sotou.flowfolk.util.provider.BuiltinUtilProvider"></bean>
						<bean class="cn.sotou.flowfolk.util.provider.GroovyUtilProvider"></bean>
						<bean class="cn.sotou.flowfolk.util.provider.BeanUtilProvider"></bean>
					</list>
				</property>
			</bean>
		</property>
	</bean>

	<bean id="pipeInterpreter" class="cn.sotou.flowfolk.interpreter.PipeInterpreter" init-method="init">
		<property name="config">
			<ref bean="interpreterConfig"></ref>
		</property>
	</bean>
</beans>
